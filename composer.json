{
    "name": "hanoivip/activity",
    "type": "library",
	"repositories" : [
		{
			"type" : "path",
			"url" : "../platform",
			"options" : {
					"symlink" : true
			}
		}, {
			"type" : "path",
			"url" : "../events",
			"options" : {
					"symlink" : true
			}
		}
	],	
    "require": {},
	"require" : {
		"php" : ">=7.0.0",
		"hanoivip/platform" : "dev-master",
		"hanoivip/events": "dev-master"
	},
	"autoload" : {
		"psr-4" : {
			"Hanoivip\\Activity\\" : "src/"
		}
	},
	"extra" : {
		"laravel" : {
			"providers" : [
				"Hanoivip\\Activity\\LibServiceProvider",
				"Hanoivip\\Activity\\EventServiceProvider"
			]
		}
	}
}
